{
  "name": "contao-themes-net/mate-theme-bundle",
  "type":"contao-bundle",
  "description": "mate theme bundle for Contao 5",
  "keywords":["contao","theme","template"],
  "homepage": "https://contao-themes.net",
  "license": "proprietary",
  "authors": [
    {
      "name": "Webdesign für Dresden",
      "homepage": "https://pdir.de",
      "email" : "develop@pdir.de",
      "role" : "Developer"
    },
    {
      "name": "Kostenlose Contao Themes",
      "homepage": "https://contao-themes.net",
      "email" : "develop@pdir.de",
      "role" : "Supplier"
    }
  ],
  "support" : {
    "docs": "https://docs.contao-themes.net/",
    "forum": "https://community.contao.org",
    "email" : "develop@pdir.de",
    "issues" : "https://github.com/contao-themes-net/mate-theme-bundle/issues",
    "source" : "https://github.com/contao-themes-net/mate-theme-bundle",
    "donate": "https://contao-themes.net/sponsoring.html"
  },
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.0",
    "contao-dd/advanced-classes-bundle": "^1.3",
    "contao-themes-net/material-icons-inserttag": "^1.2",
    "contao-themes-net/theme-components-bundle": "^1.4",
    "pdir/contao-theme-helper-bundle": "^1.4",
    "pdir/contao-webtools": "^1.5",
    "terminal42/contao-folderpage": "^3.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.5",
    "contao/manager-plugin": "^2.0",
    "contao/easy-coding-standard": "^3.0",
    "phpunit/phpunit": "^7.1 || ^8.4 || ^9.5",
    "symfony/phpunit-bridge": "^4.4 || ^5.1",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpstan/phpstan-symfony": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemesNet\\MateThemeBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "ContaoThemesNet\\MateThemeBundle\\ContaoManager\\Plugin"
  },
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/user?u=28241718"
    },
    {
      "type": "corporate",
      "url": "https://contao-themes.net/sponsoring.html"
    }
  ],
  "suggest": {
    "pdir/maklermodul-bundle": "MATE Theme includes all styles for maklermodul bundle. / MATE Theme enthält Stylesheets für das Maklermodul Bundle.",
    "pdir/social-feed-bundle": "MATE Theme includes all styles for social feed bundle. / MATE Theme enthält Stylesheets für das Social Feed Bundle",
    "pdir/animated-timeline-bundle": "Render content in a dynamic timeline. / Zeige Inhalte in einem dynamischen Zeitstrahl an."
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "contao/manager-plugin": true,
      "bamarni/composer-bin-plugin": true,
      "php-http/discovery": true
    }
  }
}
